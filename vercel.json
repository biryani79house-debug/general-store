{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "customer.html",
      "use": "@vercel/static"
    },
    {
      "src": "sales.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/customer",
      "dest": "/customer.html"
    },
    {
      "src": "/sales",
      "dest": "/sales.html"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ],
  "regions": ["sin1"]
}
